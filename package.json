  "main": "lib/index.js",
  "scripts": {
    "lint": "eslint src/*",
    "compile": "rm -rf lib/; babel --plugins transform-runtime --presets es2015,stage-0,react -d lib/ src/",
    "prepublish": "npm run compile",
    "test": "mocha --compilers js:babel-core/register"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/masotime/redouter.git"
  },
  "keywords": [
    "react",
    "react-router",
    "redux"
  ],
  "author": "Benjamin Goh <masotime@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6",
    "babel-core": "^6",
    "babel-eslint": "^6",
    "babel-plugin-transform-runtime": "^6.6.0",
    "babel-preset-es2015": "^6",
    "babel-preset-react": "^6",
    "babel-preset-stage-0": "^6",
    "history": "^2",
    "mocha": "^2.3.4",
    "react": "^15",
    "react-dom": "^15",
    "react-redux": "^4",
    "react-router": "^2",
    "redux": "^3"
  },
  "peerDependencies": {
    "history": "^2",
    "react": "^15",
    "react-redux": "^4",
    "react-router": "^2",
    "react-dom": "^15",
    "redux": "^3"
  },
  "dependencies": {
    "babel-runtime": "^6",
    "es6-promise": "^3.0.2",
